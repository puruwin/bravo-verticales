---
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header.astro";
import HeroSection from "../components/HeroSection.astro";
import TrustSection from "../components/TrustSection.astro";
import ServicesSection from "../components/ServicesSection.astro";
import WhyChooseUsSection from "../components/WhyChooseUsSection.astro";
import ConversionBlock from "../components/ConversionBlock.astro";
import ProjectsGallery from "../components/ProjectsGallery.astro";
import ContactSection from "../components/ContactSection.astro";
import Footer from "../components/Footer.astro";
import data from "../data.json";

const { empresa } = data;
---

<Layout title={empresa.titulo_completo}>
	<Header />
	<main>
		<HeroSection />
		<TrustSection />
		<ServicesSection />
		<WhyChooseUsSection />
		<ConversionBlock />
		<ProjectsGallery />
		<ContactSection />
	</main>
	<Footer />
</Layout>
